# ------------------- #
# AtennaC WebAssembly
# ------------------- #
# Makefile for the whole project generated by luxar
# ------------------------------------------------- #
#
# Compile for Node.js:	make nodejs
# Compile for Web:		make web
#
# ------------------------------------------------- #

ATENNAC=sudo atennac
all: nodejs web

wasm:	
	$(ATENNAC) src/index.atn -wasm src/index
nodejs: wasm
	$(ATENNAC) main.atn -node-js main
web: wasm
	$(ATENNAC) web/wasm.atn -web-js web/wasm